{
  "$schema": "./appsettings-schema.json",

  "Logging": {
    "LogLevel": {
      "Default": "Debug",
    },
    "Console": {
      "DisableColors": true,
      "FormatterName": "systemd",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-MM-ddTHH:mm:ss.fffffffK : ",
        "UseUtcTimestamp": true
      }
    }
  },
  "Tests": {
    "Database": {
      "DatabaseType": "SQLite",
      "PrepareThreadCount": 4,
      "SchemaDatabaseCount": 4,
      "EmptyDatabasesCount": 2,
      "SQLServerMasterConnectionString": ""
    }
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "AllowedUploadedFileExtensions": ["jpg", "png", "gif", "svg"]
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "NuCache": {
        "UsePagedSqlQuery": true,
        "UnPublishedContentCompression": true,

      }
    }
  }
}
